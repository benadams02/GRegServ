<Window x:Class="GRegServ.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GRegServ"
        mc:Ignorable="d"
        Title="GRegServ" Height="146" Width="587.5" ResizeMode="NoResize">
    <Grid Margin="0,0,0,0.2" HorizontalAlignment="Left" Width="570">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="461*"/>
            <ColumnDefinition Width="109*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnSelectFiles" Content="Select Files" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Margin="12.8,12,0,0" Height="23" Grid.Column="1" Click="btnSelectFiles_Click"/>
        <Button x:Name="btnRegister" Content="Register File" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Margin="13.2,44,0,0" Height="23" Click="btnRegister_Click" Grid.Column="1"/>
        <Button x:Name="btnUnregister" Content="Unregister File" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Margin="13.2,76,0,0" Height="23" Click="btnUnregister_Click" Grid.Column="1"/>
        <ListBox x:Name="lstSelectedFiles" HorizontalAlignment="Left" Width="441" Margin="10,12,0,9.8"/>

    </Grid>
</Window>
